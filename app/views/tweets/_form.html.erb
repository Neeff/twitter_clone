<%= form_with(model: tweet, local: true) do |form| %>



  <div class="field">
    <%= form.label :title %>
    <%= form.text_field:title ,class: 'form-control' %>
  </div>
  <div class="field">
    <%= form.label :content %>
    <%= form.text_area :content,class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn btn-success'%>
  </div>
<% end %>
